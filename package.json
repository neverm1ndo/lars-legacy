{
  "name": "lars",
  "version": "0.9.32",
  "description": "GTA-Liberty Admin Remote Service",
  "homepage": "https://github.com/neverm1ndo/lars",
  "author": {
    "name": "neverm1ndo",
    "email": "it@nmnd.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neverm1ndo/lars.git"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "neverm1ndo",
        "repo": "lars"
      }
    ]
  },
  "resolutions": {
    "webpack": "^5.0.0"
  },
  "keywords": [],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "pack": "electron-builder --dir",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href /",
    "deploy": "electron-builder build --publish always --x64 --ia32 -c ./electron-builder.json",
    "shell:dev": "ng run angular-electron:app-shell:dev",
    "shell:prod": "ng run angular-electron:app-shell:production",
    "front-deployer": "ng build --base-href=/ -c production && node deployer/bin/index.js",
    "front-deployer:only": "node deployer/bin/index.js",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c dev",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve --enable-gpu-rasterization",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build:portable": "npm run build:prod && electron-builder build -c ./electron-builder.portable.json",
    "electron:build:nsis": "electron-builder build --ia32 --x64 -c ./electron-builder.json",
    "electron:build:installer": "npm run build:prod && electron-builder build -c ./electron-builder.json",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "coverage-report": "ng test --no-watch --code-coverage"
  },
  "dependencies": {

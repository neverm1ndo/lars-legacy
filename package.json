{
  "name": "lars",
  "version": "0.9.40",
  "description": "GTA-Liberty Admin Remote Service",
  "homepage": "https://github.com/neverm1ndo/lars",
  "author": {
    "name": "neverm1ndo",
    "email": "it@nmnd.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/neverm1ndo/lars.git"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "neverm1ndo",
        "repo": "lars"
      }
    ]
  },
  "keywords": [
    "samp",
    "gta",
    "electron",
    "angular",
    "typescript",
    "windows",
    "linux"
  ],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "pack": "electron-builder --dir",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href /",
    "deploy": "electron-builder build --publish always --x64 --ia32 -c ./electron-builder.json",
    "shell:dev": "ng run angular-electron:app-shell:dev",
    "shell:prod": "ng run angular-electron:app-shell:production",
    "front-deployer": "ng build --base-href=/ -c production && node deployer/bin/index.js",
    "front-deployer:only": "node deployer/bin/index.js",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c dev",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve --enable-gpu-rasterization",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build:portable": "npm run build:prod && electron-builder build -c ./electron-builder.portable.json",
    "electron:build:nsis": "electron-builder build --ia32 --x64 -c ./electron-builder.json",
    "electron:build:installer": "npm run build:prod && electron-builder build -c ./electron-builder.json",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint",
    "coverage-report": "ng test --no-watch --code-coverage"
  },
  "dependencies": {
    "@angular/cdk": "18.1.1",
    "@angular/common": "18.1.2",
    "@angular/compiler": "18.1.2",
    "@angular/core": "18.1.2",
    "@angular/forms": "18.1.2",
    "@angular/language-service": "18.1.2",
    "@angular/platform-browser": "18.1.2",
    "@angular/platform-browser-dynamic": "18.1.2",
    "@angular/router": "18.1.2",
    "@angular/service-worker": "18.1.2",
    "@ctrl/ngx-codemirror": "4.0.1",
    "@fortawesome/angular-fontawesome": "0.11.0",
    "@fortawesome/fontawesome-svg-core": "~1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ng-bootstrap/ng-bootstrap": "17.0.0",
    "@ng-select/ng-select": "13.4.1",
    "@ngrx/effects": "18.0.1",
    "@ngrx/operators": "^18.0.0",
    "@ngrx/router-store": "18.0.1",
    "@ngrx/store": "18.0.1",
    "@ngrx/store-devtools": "18.0.1",
    "@popperjs/core": "^2.11.8",
    "@types/dat.gui": "0.7.7",
    "@types/iconv-lite": "0.0.1",
    "@types/three": "0.148.0",
    "angular-split": "14.1.0",
    "bootstrap": "^5.2.0",
    "chart.js": "^3.6.0",
    "codemirror": "5.63.3",
    "dat.gui": "0.7.9",
    "lodash-es": "4.17.21",
    "ng2-charts": "4.0.1",
    "ngx-socket-io": "4.7.0",
    "rxjs": "7.5.7",
    "three": "0.145.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.14.8"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "18.0.0",
    "@angular-devkit/build-angular": "18.1.2",
    "@angular-eslint/builder": "18.1.0",
    "@angular-eslint/eslint-plugin": "18.1.0",
    "@angular-eslint/eslint-plugin-template": "18.1.0",
    "@angular-eslint/schematics": "18.1.0",
    "@angular-eslint/template-parser": "18.1.0",
    "@angular/animations": "18.1.2",
    "@angular/cli": "18.1.2",
    "@angular/compiler-cli": "18.1.2",
    "@angular/localize": "18.1.2",
    "@ngrx/eslint-plugin": "^18.0.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@playwright/test": "1.27.1",
    "@types/jasmine": "4.3.0",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.11.0",
    "@types/ping": "0.4.1",
    "@typescript-eslint/eslint-plugin": "7.17.0",
    "@typescript-eslint/parser": "7.17.0",
    "conventional-changelog-cli": "2.2.2",
    "electron": "22.3.25",
    "electron-builder": "24.13.3",
    "electron-debug": "3.2.0",
    "electron-reloader": "1.2.3",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.8.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "4.4.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.3.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "node-polyfill-webpack-plugin": "2.0.1",
    "npm-run-all": "4.1.5",
    "playwright": "1.27.1",
    "prettier": "3.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.4.5",
    "wait-on": "7.2.0",
    "webdriver-manager": "12.1.9"
  },
  "engines": {
    "node": ">= 16.14.0 || >= 18.10.0"
  },
  "browserslist": [
    "chrome 100"
  ]
}

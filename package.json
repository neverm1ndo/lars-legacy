    "@ng-select/ng-select": "6.1.0",
    "axios": "0.25.0",
    "bootstrap": "4.5.2",
    "chart.js": "3.7.0",
    "codemirror": "5.58.2",
    "electron-tabs": "0.15.0",
    "electron-updater": "4.3.5",
    "electron-window-state": "5.0.3",
    "ng2-charts": "3.0.8",
    "ngx-indexed-db": "9.4.2",
    "ngx-socket-io": "4.1.0",
    "postcss": "8.4.5"
  }
}